<Window x:Class="FlipFlop.Interface_WPF.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlipFlop.Interface_WPF"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <Window.Resources>

        <SolidColorBrush x:Key="Background" Color="#A1D1E0" />
        <SolidColorBrush x:Key="BackgroundDark" Color="#3C93AD" />
        <SolidColorBrush x:Key="Grid" Color="#5D82C7" />
        <SolidColorBrush x:Key="GridDark" Color="#23427A" />
        <SolidColorBrush x:Key="Popup" Color="#FFD02D" />
        <SolidColorBrush x:Key="PopupDark" Color="#F1BC19" />
    </Window.Resources>


    
</Window>
